{
  "expo": {
    "name": "code-tube",
    "slug": "code-tube-slug",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/shared/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.kris2236.codetube"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "preview": {
        "distribution": "internal"
      },
      "production": {}
    }
  }
}
